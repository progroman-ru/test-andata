{
  "name": "andata/test",
  "type": "project",
  "description": "Test for Andata",
  "require": {
    "php": "^8.3",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": ["config.php", "app/utilities.php"]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
